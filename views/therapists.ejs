    <div class="dashboard-container">
        <h2>Welcome to your dashboard, Yash!</h2>
        <div class="stats-container">
          <div class="stats-box">
            <h3>7</h3>
            <p>Active Clients</p>
          </div>
          <div class="stats-box">
            <h3>23</h3>
            <p>Completed Appointments</p>
          </div>
          <div class="stats-box">
            <h3>4.8</h3>
            <p>Average Rating</p>
          </div>
        </div>
        
      </div>
      <div style="text-align: center;">
      <button id="set-availability-btn">Set Availability</button>
    </div>
      <div class="appointments-container">


        <div class="appointments-list">
          <h3>Pending Appointments:</h3>
          <ul>
            <% for(let i=0;i<PendingList.length;i++){ %>
            <li><%=PendingList[i]%> <button class="accept">Accept</button> <button class="cancel">Cancel</button></li>
            <% } %>
          </ul>
        </div>
        
        
        <div class="appointments-list">
          <h3>Upcoming Appointments:</h3>
          <ul>
            <li>[client name] - [appointment date and time] </li>
            <li>[client name] - [appointment date and time] </li>
            <li>[client name] - [appointment date and time] </li>
          </ul>
        </div>
      </div>


    

<div class="modal">
  <div class="modal-content">
    <span class="close">&times;</span>
    <h2>Select your available time slots</h2>
    <form class="available-form" action="/availability" method="post">
      <label for="availability-date">Select a date:</label>
      <input type="date" id="availability-date" name="availability_date" required>
      <label for="availability-time">Select time slots:</label>
      <div class="checkbox-group" name="time_slot">
        <label><input type="checkbox" name="availability_time" value="9:00 AM - 11:00 AM">9:00 AM - 11:00 AM</label>
        <label><input type="checkbox" name="availability_time" value="12:00 PM - 02:00 PM">12:00 PM - 02:00 PM</label>
        <label><input type="checkbox" name="availability_time" value="03:00 PM - 05:00 PM">03:00 PM - 05:00 PM</label>
        <label><input type="checkbox" name="availability_time" value="06:00 PM - 08:00 PM">06:00 PM - 08:00 PM</label>
        <label><input type="checkbox" name="availability_time" value="09:00 PM : 11:00 PM">09:00 PM : 11:00 PM</label>
        
      </div>
      <button type="submit">Save Availability</button>
    </form>
  </div>
</div>
      
      



